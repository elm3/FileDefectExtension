{
  "name": "ButtonAdd",
  "version": "0.0.2",
  "description": "Add a button to create a ticket.",
  "permissions": [
    "activeTab",
    "storage",
    "tabs"
  ],
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "content_scripts": [
    {
      "matches": [ "https://os.mbed.com/questions/*" ],
      "js": [
        "jquery-3.3.1.min.js",
        "contentScript.js"
      ]
    }
  ],
  "icons": {
    "16": "images/get_started16.png",
    "32": "images/get_started32.png",
    "48": "images/get_started48.png",
    "128": "images/get_started128.png"
  },
  "manifest_version": 2
}
